timezone: Asia/Tokyo

+bizdate_setup:
  sh>: stfw bizdate --setup "${stfw_bizdate_dir}"

+bizdate_teardown:
  _export:
    stfw_run_status : "Success"
  sh>: stfw bizdate --teardown "${stfw_bizdate_dir}"

_error:
  _export:
    stfw_run_status : "Error"
  sh>: stfw bizdate --teardown "${stfw_bizdate_dir}"
